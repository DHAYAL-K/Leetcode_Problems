class Solution {
public:
    int minOperations(vector<int>& nums) {
        map<int,int>mpp;
        for(int i=0;i<nums.size();i++){
            mpp[nums[i]]++;
        }
        int ans=0;
        for(auto it:mpp){
            if(it.second==1)return -1;
            ans+=it.second/3 + (it.second % 3 >0);
        }
        return ans;
    }
};
