class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        int answer;
        vector<vector<int>> ans;
        for(int i=1;i<=numRows;i++){
            vector<int>temp;
            answer=1;
            temp.push_back(answer);
            for(int j=1;j<i;j++){
                answer=answer*(i-j);
                answer=answer/j;
                temp.push_back(answer);
            }
            ans.push_back(temp);
        }
        return ans;
    }

};
