class Solution {
public:
    int countMatches(vector<vector<string>>& items, string ruleKey, string ruleValue) {
        int rows=items.size();
        int count=0;
        int temp=0;
                if(ruleKey=="type")temp=0;
                else if(ruleKey=="color")temp=1;
                else temp=2;
        for(int i=0;i<rows;i++){
            if(items[i][temp]==ruleValue)count++;
        }
        return count;
    }
};
